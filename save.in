                } else {
                    freq[j] += word2[j];
                }
            }
        }

        for(int i = 0; i < freq.length; i++) {
            out.println(freq[i]);
        }
        out.close();
    }

    public static int[] howManyChar(String str) {
        int[] freq = new int[26];
        for(int i = 0; i < str.length(); i++) {
            int index = str.charAt(i) - 'a';
            freq[index]++;
        }
        return freq;
    }
